<?xml version="1.0" encoding="UTF-8"?>
<xrds:XRDS xmlns:xrds="xri://$xrds" xmlns="xri://$xrd*($v*2.0)">
<XRD>
    {% for service in services %}
    <Service priority="{{forloop.counter0}}">
        <Type>{{service.type}}</Type>
        <URI>{{service.uri}}</URI>
    </Service>
    {% endfor %}
</XRD>
</xrds:XRDS>
